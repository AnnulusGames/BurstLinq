            else if (BurstHelpers.IsV128Supported)
            {
                const int packingLength = 16 / sizeof(<#=type#>);
              
                static bool<#=vectorLength/2#> _contains(v128 a, <#=type#>* b) => new bool<#=vectorLength/2#>(
<# for (int i = 0; i < vectorLength / 2 - 1; i++) {#>
