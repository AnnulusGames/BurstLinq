            return ContainsCore((<#=intType#>*)ptr, length, union);
        }
<#} else if (IsPrimitive(type)) { #>
<#var vName = V256Name(type);#>
        [BurstCompile]
        internal static bool ContainsCore(<#=type#>* ptr, [AssumeRange(1, int.MaxValue)] int length, in <#=type#> value)
        {
            var index = 0;
