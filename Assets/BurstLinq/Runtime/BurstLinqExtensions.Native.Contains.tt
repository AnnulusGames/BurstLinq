        {
            return ContainsCore((<#=type#>*)source.GetUnsafePtr(), source.Length, value);
        }

<#if (TryGetInteger(type, out var intType)){#>
        internal static bool ContainsCore(<#=type#>* ptr, [AssumeRange(1, int.MaxValue)] int length, in <#=type#> value)
        {
